<launch>

    <arg name="model" />
    <arg name="init_pose" />

    <param name="robot_description" command="xacro '$(find machine_vision_bonus)/urdf/$(arg model)/robot.xacro'" />

    <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" respawn="false" 
        args="$(arg init_pose) -urdf -model $(arg model) -param robot_description" />
    <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher"/>
    <node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher" />
</launch>